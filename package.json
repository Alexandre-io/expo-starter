{
  "name": "expo-starter",
  "version": "2.0.0",
  "description": "Production-ready starter for Expo (React Native) App! Powered by cli-rn, React Navigation (v6), RN UI lib, Mobx, Reanimated 2, Dark Mode, Localization, and much more.",
  "license": "MIT",
  "author": "Batyr <dev@batyr.io>",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "p:web": "expo build:web && cd web-build && vercel --prod && cd ..",
    "p:app": "expo p",
    "p:all": "yarn p:app && yarn p:web",
    "format": "prettier --check ./src",
    "format:write": "prettier --write ./src",
    "release": "dotenv release-it"
  },
  "dependencies": {
    "@kanzitelli/if-component": "^0.1.1",
    "@react-native-async-storage/async-storage": "~1.15.0",
    "@react-native-community/datetimepicker": "3.5.2",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/netinfo": "6.0.0",
    "@react-native-picker/picker": "1.16.1",
    "@react-navigation/bottom-tabs": "^6.0.7",
    "@react-navigation/native": "^6.0.4",
    "@react-navigation/native-stack": "^6.2.2",
    "@react-navigation/stack": "^6.0.9",
    "date-fns": "^2.24.0",
    "expo": "^42.0.4",
    "expo-localization": "~10.2.0",
    "expo-status-bar": "~1.0.4",
    "expo-updates": "~0.8.2",
    "formik": "^2.2.9",
    "i18n-js": "^3.8.0",
    "lodash": "^4.17.21",
    "mobx": "^6.3.3",
    "mobx-persist-store": "^1.0.4",
    "mobx-react": "^7.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-ui-lib": "^6.0.0",
    "react-native-web": "~0.13.12",
    "rn-bounceable": "^0.1.0"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/i18n-js": "^3.8.2",
    "@types/react": "~16.9.35",
    "@types/react-dom": "~16.9.8",
    "@types/react-native": "~0.63.2",
    "@typescript-eslint/parser": "^4.32.0",
    "dotenv-cli": "^4.0.0",
    "prettier": "^2.4.1",
    "release-it": "^14.11.6",
    "typescript": "~4.0.0"
  }
}
